<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" >
    <title>@ViewData["Title"] - ExamensArbeteMVCApp2</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true" >
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" >
    <link rel="stylesheet" href="~/ExamensArbeteMVCApp2.styles.css" asp-append-version="true" >
    <link href="~/img/favicon.ico" rel="icon" asp-append-version="true">
    <link rel="stylesheet" href="//cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" />


    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" asp-append-version="true">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin asp-append-version="true">
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@500;600;700&family=Open+Sans:wght@400;500&display=swap" rel="stylesheet" asp-append-version="true">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet" asp-append-version="true">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet" asp-append-version="true">

    <!-- Libraries Stylesheet -->
    <link href="~/lib/animate/animate.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/lib/lightbox/css/lightbox.min.css" rel="stylesheet" asp-append-version="true">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">
</head>
<body>
    <div class="container-fluid" style="width: 100%">
        <main role="main" class="pb-3" container>
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous" asp-append-version="true"></script>
    <script src="~/lib/wow/wow.min.js" asp-append-version="true"></script>
    <script src="~/lib/easing/easing.min.js" asp-append-version="true"></script>
    <script src="~/lib/waypoints/waypoints.min.js" asp-append-version="true"></script>
    <script src="~/lib/owlcarousel/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/lib/counterup/counterup.min.js" asp-append-version="true"></script>
    <script src="~/lib/parallax/parallax.min.js" asp-append-version="true"></script>
    <script src="~/lib/isotope/isotope.pkgd.min.js" asp-append-version="true"></script>
    <script src="~/lib/lightbox/js/lightbox.min.js" asp-append-version="true"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    <script src="//cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/plug-ins/1.10.16/sorting/datetime-moment.js"></script>

    <script type="text/javascript" src="https://cdn.datatables.net/plug-ins/1.10.21/dataRender/datetime.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
